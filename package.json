{
  "name": "skill-stream",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "start:prod": "turbo run start:prod",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:cov": "turbo run test:cov",
    "clean": "turbo clean && pnpm -r exec rm -rf node_modules/.vite",
    "backend:dev": "pnpm --filter skill-stream-backend dev",
    "backend:build": "pnpm --filter skill-stream-backend build",
    "backend:start": "pnpm --filter skill-stream-backend start",
    "backend:start:prod": "pnpm --filter skill-stream-backend start:prod",
    "backend:lint": "pnpm --filter skill-stream-backend lint",
    "backend:format": "pnpm --filter skill-stream-backend format",
    "backend:test": "pnpm --filter skill-stream-backend test",
    "frontend:dev": "pnpm --filter skill-stream-frontend dev",
    "frontend:build": "pnpm --filter skill-stream-frontend build",
    "frontend:preview": "pnpm --filter skill-stream-frontend preview",
    "frontend:lint": "pnpm --filter skill-stream-frontend lint",
    "frontend:format": "pnpm --filter skill-stream-frontend format"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}